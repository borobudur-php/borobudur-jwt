{
    "name": "borobudur/jwt",
    "description": "Borobudur JWT Encryption Component",
    "keywords": ["borobudur", "jwt", "Json Web Tokens"],
    "type": "borobudur-component",
    "license": "MIT",
    "homepage": "https://github.com/borobudur-php/borobudur-jwt",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
